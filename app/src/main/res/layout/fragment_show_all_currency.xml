<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="propertyMoney"
            type="com.aliagushutapea.convertion.money_exchange.PropertyMoney" />

        <variable
            name="presenter"
            type="com.aliagushutapea.convertion.money_exchange.MoneyExchangeFragmentPresenter" />

    </data>

    <RelativeLayout
        android:id="@+id/relatiflayout_container_show_all_currency"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#fff">

        <include
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            layout="@layout/default_toolbar" />

        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerViewListCurrency"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/toolbar"
            />

    </RelativeLayout>
</layout>
